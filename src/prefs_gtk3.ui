<?xml version="1.0" encoding="UTF-8"?>
<interface domain="gnome-shell-extension-simple-system-monitor">
  <requires lib="gtk+" version="3.20" />
  <object class="GtkAdjustment" id="refresh_rate_adjustment">
    <!-- 86400 seconds = 24 * 60 * 60 = 1 day -->
    <property name="upper">86400</property>
    <property name="lower">1</property>
    <property name="step-increment">1</property>
    <property name="page-increment">5</property>
  </object>
  <object class="GtkAdjustment" id="font_weight_adjustment">
    <property name="upper">900</property>
    <property name="lower">100</property>
    <property name="step-increment">100</property>
  </object>
  <object class="GtkAdjustment" id="extension_order_adjustment">
    <property name="upper">255</property>
    <property name="lower">-255</property>
    <property name="step-increment">1</property>
  </object>
  <object class="GtkListStore" id="extension_position_list_store">
    <columns>
      <column type="gchararray" />
    </columns>
    <data>
      <row>
        <col id="0">Left</col>
      </row>
      <row>
        <col id="0">Center</col>
      </row>
      <row>
        <col id="0">Right</col>
      </row>
    </data>
  </object>
